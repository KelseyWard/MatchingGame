<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zybooks.matchinggame"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.MatchingGame" >
        <activity
            android:name="com.zybooks.matchinggame.information"
            android:exported="true" />
        <activity
            android:name="com.zybooks.matchinggame.chooseLevel"
            android:exported="true" />
        <activity
            android:name="com.zybooks.matchinggame.MediumLevel"
            android:exported="true" />
        <activity
            android:name="com.zybooks.matchinggame.EasyLevel"
            android:exported="true" />
        <activity
            android:name="com.zybooks.matchinggame.title_screen"
            android:exported="true" />
        <activity
            android:name="com.zybooks.matchinggame.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.zybooks.matchinggame.SplashScreen"
            android:exported="true"
            android:theme="@style/splashScreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>